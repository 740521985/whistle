{
  "name": "whistle",
  "description": "web proxy",
  "version": "0.2.3",
  "dataDirname": ".whistle", 
  "localUIHost": "local.whistlejs.com",
  "whistleSsl": "whistle-ssl",
  "port": 8899,
  "ports": ["uiport", "uidataport", "weinreport"],
  "sockets": 8,
  "keepAlive": true,
  "keepAliveMsecs": 10000,
  "timeout": 36000,
  "httpsProxyHost": "x-whistle-proxy-host",
  "author": "avenwu <avenwu@vip.qq.com>",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/avwo/whistle",
  "keywords": ["proxy", "fiddler", "charles"],
  "bin": {
    "whistle": "./bin/whistle.js"
  },
  "registry": "http://registry.npmjs.org/whistle",
  "repository": {
	  "type": "git",
	  "url": "https://github.com/avwo/whistle.git"
  },
  "dependencies": {
	   "express": "4.12.3",
	   "body-parser": "1.12.0",
	   "basic-auth": "1.0.0",
	   "mime": "1.3.4",
	   "iconv-lite": "0.4.7",
	   "weinre": "2.0.0-pre-I0Z7U9OV",
	   "q": "1.4.1",
	   "pipestream": "*",
	   "starting": ">=0.0.9",
	   "node-forge": "0.6.34",
	   "socksv5": "0.0.6",
	   "fs-extra": "0.22.1"
  },
  "scripts": {
		"start": "node app.js"
},
  "engines": {
    "node": ">= 0.10.0"
  }
}
